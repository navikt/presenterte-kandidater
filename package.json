{
    "private": true,
    "sideEffects": false,
    "scripts": {
        "build": "remix build && run-p build:server",
        "build:server": "tsc --project server/tsconfig.json",
        "dev": "remix build && run-p \"dev:*\"",
        "dev:node": "cross-env NODE_ENV=development nodemon ./server/server.ts --watch ./server/server.ts",
        "dev:remix": "remix watch",
        "start": "cross-env NODE_ENV=production ts-node ./server/server.ts",
        "format": "prettier --write .",
        "prepare": "husky install"
    },
    "dependencies": {
        "@navikt/ds-css": "^1.3.18",
        "@navikt/ds-icons": "^1.3.18",
        "@navikt/ds-react": "^1.3.18",
        "@remix-run/express": "^1.7.2",
        "@remix-run/node": "^1.7.2",
        "@remix-run/react": "^1.7.2",
        "compression": "^1.7.4",
        "cross-env": "^7.0.3",
        "express": "^4.18.1",
        "isbot": "^3.5.4",
        "morgan": "^1.10.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.7.2",
        "@remix-run/eslint-config": "^1.7.2",
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.14",
        "@types/morgan": "^1.9.3",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "eslint": "^8.23.1",
        "husky": "^8.0.0",
        "nodemon": "^2.0.19",
        "npm-run-all": "^4.1.5",
        "pretty-quick": "^3.1.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    },
    "engines": {
        "node": ">=14"
    },
    "prettier": {
        "tabWidth": 4,
        "trailingComma": "es5",
        "arrowParens": "always",
        "printWidth": 100
    }
}
